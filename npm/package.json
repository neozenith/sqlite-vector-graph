{
  "name": "sqlite-muninn",
  "version": "0.1.0-alpha.9",
  "description": "HNSW vector search + graph traversal + Node2Vec for SQLite",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "keywords": ["sqlite", "vector", "hnsw", "graph", "node2vec", "search"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neozenith/sqlite-muninn.git"
  },
  "scripts": {
    "build": "tsup",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/",
    "VERSION"
  ],
  "optionalDependencies": {
    "@sqlite-muninn/darwin-arm64": "0.1.0-alpha.9",
    "@sqlite-muninn/darwin-x64": "0.1.0-alpha.9",
    "@sqlite-muninn/linux-x64": "0.1.0-alpha.9",
    "@sqlite-muninn/linux-arm64": "0.1.0-alpha.9",
    "@sqlite-muninn/win32-x64": "0.1.0-alpha.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
